{
  "id": "dev.pelican.panel",
  "title": "Pelican Panel",
  "version": "1.0.0-beta22",
  "description": "Open-source game server management panel",
  "tagline": "Manage your game servers with Pelican",
  "website": "https://pelican.dev",
  "icon": "logo.png",
  "healthCheckPath": "/",
  "httpPort": 80,
  "manifestVersion": 2,
  "dockerImage": "ghcr.io/orcvole/panel:latest",
  "addons": {
    "localstorage": {},
    "mysql": {},
    "sendmail": {}
  },
  "postInstallMessage": "After installation, please visit https://${DOMAIN}/installer to complete the setup.",
  "environment": [
    {
      "key": "APP_ENV",
      "value": "production"
    },
    {
      "key": "APP_DEBUG",
      "value": "false"
    },
    {
      "key": "APP_URL",
      "value": "https://${APP_DOMAIN}"
    },
    {
      "key": "APP_KEY",
      "value": "base64:z6yFDeTUxbHvnHipBHO1FMJIyDntdtufLBsG84MUEAQ="
    },
    {
      "key": "DB_CONNECTION",
      "value": "mysql"
    },
    {
      "key": "DB_HOST",
      "value": "${MYSQL_HOST}"
    },
    {
      "key": "DB_PORT",
      "value": "3306"
    },
    {
      "key": "DB_DATABASE",
      "value": "${MYSQL_DATABASE}"
    },
    {
      "key": "DB_USERNAME",
      "value": "${MYSQL_USERNAME}"
    },
    {
      "key": "DB_PASSWORD",
      "value": "${MYSQL_PASSWORD}"
    },
    {
      "key": "MAIL_DRIVER",
      "value": "sendmail"
    },
    {
      "key": "MAIL_FROM_ADDRESS",
      "value": "admin@${APP_DOMAIN}"
    },
    {
      "key": "MAIL_FROM_NAME",
      "value": "Pelican Panel"
    }
  ]
}

